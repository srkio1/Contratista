<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Contratista.Busqueda">
    <ContentPage.Content>
        <StackLayout>
            <SearchBar Placeholder="Search..." x:Name="sb_search"></SearchBar>
            <ListView x:Name="listSearch">
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <TextCell Text="{Binding nombre}" Detail="{Binding telefono}">
                        </TextCell>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>